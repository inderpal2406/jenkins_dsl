pipeline {
    agent any
   
    stages {
        stage('Build') {
            steps {
                echo "This is build stage."
                echo "Building..."
            }
        }
        stage('Test') {
            steps {
                echo "This is test stage."
                echo "Testing..."
            }
        }
        stage('Deploy') {
            steps {
                echo "This is deploy stage."
                echo "Deploying..."
            }
        }
    }
}
